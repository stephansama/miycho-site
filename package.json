{
	"displayName": "MIYCHO",
	"name": "portfolio",
	"type": "module",
	"version": "0.0.1",
	"readme": "README.md",
	"license": "Proprietary works of Stephan Randle",
	"homepage": "https://stephanrandle.dev",
	"description": "My personal portfolio website built with Astro.",
	"repository": {
		"url": "https://github.com/stephansama/portfolio"
	},
	"author": {
		"name": "Stephan Randle",
		"email": "stephanrandle.dev@gmail.com",
		"url": "https://stephanrandle.dev"
	},
	"scripts": {
		"dev": "tinacms dev -c \"astro dev --host\"",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "netlify dev",
		"astro": "astro"
	},
	"browserslist": [
		"defaults and fully supports es6-module",
		"maintained node versions"
	],
	"eslintConfig": {
		"env": {
			"astro/astro": true,
			"browser": true,
			"es2021": true,
			"node": true
		},
		"ignorePatterns": [
			"postcss.config.cjs"
		],
		"extends": [
			"eslint:recommended",
			"plugin:astro/recommended",
			"plugin:astro/jsx-a11y-recommended",
			"plugin:@typescript-eslint/recommended",
			"prettier"
		],
		"plugins": [
			"@typescript-eslint"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"overrides": [
			{
				"files": [
					"*.astro"
				],
				"parser": "astro-eslint-parser",
				"rules": {
					"astro/no-set-html-directive": "off"
				},
				"parserOptions": {
					"parser": "@typescript-eslint/parser",
					"extraFileExtensions": [
						".astro"
					]
				}
			}
		],
		"rules": {
			"@typescript-eslint/no-explicit-any": "warn",
			"@typescript-eslint/triple-slash-reference": "off"
		}
	},
	"prettier": {
		"semi": true,
		"useTabs": true,
		"singleQuote": false,
		"jsxSingleQuote": false,
		"astroAllowShorthand": true,
		"trailingComma": "es5",
		"quoteProps": "consistent",
		"overrides": [
			{
				"files": "*.astro",
				"options": {
					"parser": "astro"
				}
			},
			{
				"files": "SEO.astro",
				"options": {
					"printWidth": 99
				}
			}
		],
		"pluginSearchDirs": false,
		"plugins": [
			"prettier-plugin-tailwindcss",
			"prettier-plugin-astro"
		]
	},
	"dependencies": {
		"@alpinejs/persist": "^3.13.3",
		"@astrojs/check": "^0.3.4",
		"@astrojs/react": "^3.0.9",
		"@astrojs/tailwind": "^5.0.4",
		"@iconify-json/heroicons": "^1.1.19",
		"@iconify-json/simple-icons": "^1.1.86",
		"@netlify/functions": "^2.4.1",
		"alpinejs": "^3.13.3",
		"astro": "^4.0.7",
		"astro-robots-txt": "^1.0.0",
		"figlet": "^1.7.0",
		"gsap": "^3.12.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.4.0",
		"tinacms": "^1.5.28",
		"tippy.js": "^6.3.7",
		"typescript": "^5.3.3",
		"xml": "^1.0.1"
	},
	"devDependencies": {
		"@astrojs/ts-plugin": "^1.3.1",
		"@fontsource/rubik-mono-one": "^5.0.18",
		"@iconify/tailwind": "^0.1.4",
		"@iconify/types": "^2.0.0",
		"@iconify/utils": "^2.1.13",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@tinacms/cli": "^1.5.39",
		"@types/alpinejs": "^3.13.5",
		"@types/figlet": "^1.5.8",
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@types/xml": "^1.0.11",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"clsx": "^2.0.0",
		"cssnano": "^6.0.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-astro": "^0.31.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"netlify-cli": "^17.10.1",
		"postcss-import": "^15.1.0",
		"postcss-mixins": "^9.0.4",
		"postcss-preset-env": "^9.3.0",
		"prettier": "2.8.0",
		"prettier-plugin-astro": "0.8.1",
		"prettier-plugin-tailwindcss": "0.4.1",
		"tailwind-merge": "^2.2.0",
		"tailwind-scrollbar": "^3.0.5",
		"tailwindcss-debug-screens": "^2.2.1"
	}
}
